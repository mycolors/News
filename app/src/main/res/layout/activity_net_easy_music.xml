<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fengniao.news.module.test.otherapp.neteasymusic.NetEasyMusicActivity">

    <ImageView
        android:id="@+id/iv_music_one"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="bottom"
        android:src="@drawable/img_neteasy_music"
        android:transitionName="imgOne" />

    <ImageView
        android:id="@+id/iv_music_two"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="bottom|end"
        android:src="@drawable/img_neteasy_pic"
        android:transitionName="imgTwo" />


</FrameLayout>
